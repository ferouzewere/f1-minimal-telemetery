{
  "name": "tmp-app",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "dev": "npm run dev --workspace=@app/frontend",
    "bridge": "python apps/bridge/main.py",
    "start": "concurrently \"npm run dev\" \"npm run bridge\"",
    "build": "npm run build --workspace=@app/frontend",
    "lint": "npm run lint --workspace=@app/frontend",
    "preview": "npm run preview --workspace=@app/frontend"
  },
  "dependencies": {
    "@visx/visx": "^3.12.0",
    "d3": "^7.9.0",
    "framer-motion": "^12.29.0",
    "lucide-react": "^0.563.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/d3": "^7.4.3",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "concurrently": "^9.1.2",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "rollup-plugin-visualizer": "^6.0.5",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4"
  }
}